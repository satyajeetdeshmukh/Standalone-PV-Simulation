          }
          Parameter {
            Variable      "Declarations"
            Value         "/** Bipolar PWM Version 1.0, 08-20-14\n"
" *Note that multiple sample times are used.\n"
" *The fixed sample time causes a hit at the beginning of each switching cycle"
", i.e. a hit\n"
" *is generated at n*Ts.\n"
" *The variable sample time is used to cause a hit at the PWM transitions with"
"in the cycle.\n"
" */\n"
"\n"
"#include <float.h>\n"
"#include <stdio.h>\n"
"\n"
"#define M InputSignal(0,0)\t\t//modulation index\n"
"static double TS;      \t\t\t//sample period \n"
"#define OUT OutputSignal(0,0)\t//switch output\n"
"#define STATE DiscState(0)\n"
"#define NEVER DBL_MAX\n"
"#define DMIN 0.1\n"
"\n"
"//State names\n"
"typedef enum {\n"
"  Start,\n"
"  Low,\n"
"  High,\n"
"} state_type;\n"
"\n"
"//Global variable\n"
"state_type NEXT_STATE;"
            Show          off
          }
          Parameter {
            Variable      "StartFcn"
            Value         "TS = ParamRealData(0,0);  //sample period \n"
"STATE = Start;\n"
"NextSampleHit = NEVER;"
            Show          off
          }
          Parameter {
            Variable      "OutputFcn"
            Value         "/**\n"
" *If 0 < M1 < 1, a pwm switching sequence will be generated in the order 1,0,"
"1. \n"
" */\n"
"double m1, m2;\n"
"static double t1, t2;\n"
"\n"
"switch ((state_type) STATE)\n"
"{\n"
"  //----------------------------------------\n"
"  case Start:  //Start is entered at the hit caused by the fixed step setting"
"\n"
"    \n"
" \t\n"
"\n"
"  //Scale mod index between [0 1].\n"
"  m1 = M;\n"
"  m2 = 1-m1;\n"
"  if (m1 < DMIN) \n"
"  {\n"
"    OUT = 0;\n"
"    NEXT_STATE = Start;\n"
"  }\n"
"  else if (m2 < DMIN)\n"
"  {\n"
"    OUT = 1;\n"
"    NEXT_STATE = Start;\n"
"  }\n"
"  else \n"
"  {\n"
"    OUT = 1;\n"
"    t1 = CurrentTime + TS*m1/2; //hit time for pwm hi to low\n"
"    t2 = t1 + TS*m2; //hit time for pwm low to high\n"
"    NEXT_STATE = Low;\n"
"    NextSampleHit = t1;\n"
"  }\n"
"  break;\n"
"\n"
"  //----------------------------------------\n"
"  //The states Low and High are entered at the hits caused by the variable st"
"ep setting\n"
"  case Low:\n"
"    // Output 0, advance next state and transition time\n"
"    OUT = 0;\n"
"    NEXT_STATE = High;\n"
"    NextSampleHit = t2;\n"
"    break;\n"
"\n"
"  //----------------------------------------\n"
"  case High:\n"
"    // Output 1, next state is the start of a new cycle\n"
"    OUT = 1;\n"
"    NEXT_STATE = Start;\n"
"    NextSampleHit = NEVER; //Wait for next fixed sample step hit\n"
"    break;\n"
"\n"
"}"
            Show          off
          }
          Parameter {
            Variable      "UpdateFcn"
            Value         "STATE = NEXT_STATE;"
