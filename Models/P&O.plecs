"#define I Input(1)\n"
"#define I_bat Input(2)\n"
"#define V_dc Input(3)\n"
"#define P_old DiscState(0)\n"
"#define Dref DiscState(1)\n"
"#define inc DiscState(2)\n"
"#define MPPT_Mode DiscState(3)\n"
"#define ki_term DiscState(4)\n"
"#define D_MAX 0.8\n"
"#define D_MIN 0.05\n"
"#define DeltaD_Max 0.01\n"
"#define I_bat_MAX -2\n"
"#define V_dc_ref 72\n"
"#define P_Load 200\n"
"#define kp 0.001\n"
"#define ki 0.001\n"
"\n"
"//Global variable\n"
"static double P;\n"
"static double DeltaD;\n"
"static double kp_term;\n"
                Show          off
              }
              Parameter {
                Variable      "StartFcn"
                Value         "P_old = 0;\n"
"Dref = 0.5;\n"
"inc=-1;\n"
"MPPT_Mode = 1;\n"
"ki_term = 0;\n"
                Show          off
              }
              Parameter {
                Variable      "OutputFcn"
                Value         "P = V*I;\n"
"\n"
"if(I_bat < 1.2*I_bat_MAX && P > P_Load){\n"
"\tMPPT_Mode = 0;\n"
"\tinc = 1;\n"
"}\n"
"\n"
"if(MPPT_Mode == 1){\n"
"\t// MPPT Mode\n"
"\tif(P<P_old)\n"
"\t{\n"
"\t   inc=-inc;\n"
"\t\tDeltaD=DeltaD_Max/100;\n"
"\t\tif(P-P_old > P*0.1 || P_old-P > P*0.1){\n"
"\t\t\tDeltaD=DeltaD_Max;\n"
"\t\t}\n"
"\t}\n"
"\n"
"\tDref=Dref+inc*DeltaD;\n"
"}\n"
"else if(MPPT_Mode == 0){\n"
"\t// Non-MPPT Mode\n"
"\tdouble e;\n"
"\te = I_bat - I_bat_MAX;\n"
"\tkp_term = e*kp;\n"
"\tki_term = ki_term*0.99 + e*ki;\n"
"\tDeltaD = (kp_term + ki_term)*0.0001;\n"
"\n"
"\tDref=Dref+inc*DeltaD;\n"
"}\n"
"\n"
"\n"
"\n"
"\n"
"\n"
"\n"
"if (Dref>D_MAX)\n"
"{\n"
"\tDref=D_MAX;\n"
"}\n"
"else if (Dref<D_MIN)\n"
"{\n"
"\tDref=D_MIN;\n"
"}\n"
"\n"
"Output(0) = Dref;\n"
"\n"
